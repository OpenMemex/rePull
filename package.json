{
  "name": "repull",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:renderer": "webpack --config ./config/webpack/renderer.config.js",
    "build:main": "webpack --config ./config/webpack/main.config.js",
    "build:main:prod": "npm run build:main -- --env.prod",
    "build:main:dev": "npm run build:main -- --env.dev",
    "build:preload": "webpack --config ./config/webpack/preload.config.js",
    "build:preload:prod": "npm run build:preload -- --env.prod",
    "build:preload:dev": "npm run build:preload -- --env.dev",
    "build": "npm run build:prod",
    "build:dev": "concurrently \"npm run build:main:dev\" \"npm run build:renderer\" \"npm run build:preload:dev\"",
    "build:prod": "concurrently \"npm run build:main:prod\" \"npm run build:renderer\" \"npm run build:preload:prod\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:main:dev && concurrently \"npm run build:preload:dev -- --watch\" \"npm run build:renderer -- --watch\" \"electron build/main/electron.js\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "concurrently": "5.1.0",
    "electron": "^8.1.0",
    "electron-builder": "22.4.1",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-regular-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@koa/router": "8.0.8",
    "@material-ui/core": "4.9.5",
    "clsx": "1.1.0",
    "immer": "6.0.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "simple-git": "1.131.0",
    "uuid": "7.0.2"
  },
  "build": {
    "appId": "org.repull",
    "files": ["build/**/*"]
  }
}
